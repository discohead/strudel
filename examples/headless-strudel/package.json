{
  "name": "headless-strudel",
  "version": "1.0.0",
  "description": "Headless Strudel composition environment for VS Code/Cursor",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "setup": "node setup.js",
    "start": "node index.js",
    "dev": "node --watch index.js",
    "pattern": "node --watch pattern-runner.js",
    "audio": "node audio-runner.js",
    "audio:dev": "node --watch audio-runner.js",
    "audio:simple": "node audio-runner-simple.js",
    "web": "node headless-web.js",
    "background": "node background-web.js"
  },
  "dependencies": {
    "@strudel/core": "workspace:*",
    "@strudel/mini": "workspace:*",
    "@strudel/transpiler": "workspace:*",
    "@strudel/tonal": "workspace:*",
    "@strudel/webaudio": "workspace:*",
    "@strudel/osc": "workspace:*",
    "chokidar": "^3.5.3",
    "chalk": "^5.3.0",
    "osc-js": "^2.4.0",
    "puppeteer": "^21.0.0",
    "vite": "^5.0.0"
  }
}